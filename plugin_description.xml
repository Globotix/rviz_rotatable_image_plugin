<library path="librviz_rotatable_image_plugin">
  <class name="rviz/ImageRotated" type="rviz::ImageDisplay2" base_class_type="rviz::Display">
    <description>
      Displays an image from a sensor_msgs/Image topic, similar to image_view, but allow for rotation.  &lt;a href="https://github.com/LKSeng/rviz_rotatable_image_plugin"&gt;More Information&lt;/a&gt;.
    </description>
    <message_type>sensor_msgs/Image</message_type>
    <message_type>sensor_msgs/CompressedImage</message_type>
    <message_type>theora_image_transport/Packet</message_type>
  </class>
</library>
